<b:if cond='data:blog.metaDescription'>
	<meta expr:content='data:blog.metaDescription' property='og:description'/>
	<b:else/>
	<!-- jika deskripsi tidak ditemukan boleh di isi sendiri -->
	<meta content='Tempat Situs Link dan Bookmark Seputar Bisnis Blogger, SEO, Website, dan Pemrograman, Ditambah Situs Situs Kesukaan Agar Tidak Hilang' property='og:description'/>
</b:if>
<b:if cond='data:blog.pageType == &quot;item&quot;'>
	<meta expr:content='data:blog.pageName' property='og:title'/>
	<meta content='article' property='og:type'/>
	<b:else/>
	<!-- jika bukan dihalaman item -->
	<meta expr:content='data:blog.pageName' property='og:title'/>
	<meta content='website' property='og:type'/>
</b:if>
<meta expr:content='data:blog.canonicalUrl' property='og:url'/>
<b:if cond='data:blog.postImageUrl'>
	<meta expr:content='data:blog.postImageUrl' property='og:image'/>
	<meta content='500' property='og:image:width'/>
	<meta content='500' property='og:image:height'/>
	<b:else/>
	<b:if cond='data:blog.postImageThumbnailUrl'>
		<meta expr:content='data:blog.postThumbnailUrl' property='og:image'/>
		<meta content='500' property='og:image:width'/>
		<meta content='500' property='og:image:height'/>
		<b:else/>
		<meta expr:content='data:blog.blogspotFaviconUrl' property='og:image'/>
		<meta content='500' property='og:image:width'/>
		<meta content='500' property='og:image:height'/>
	</b:if>
</b:if>
<meta expr:content='data:blog.canonicalHomepageUrl' property='og:url'/>
<meta expr:content='data:blog.title' property='og:site_name'/>
<meta content='en_US' property='og:locale'/>
<meta content='en_GB' property='og:locale:alternate'/>
<meta content='id_ID' property='og:locale:alternate'/>

<!-- google meta tag -->
<link href='https://plus.google.com/112970180154389856347/posts' rel='publisher'/>
<link href='https://plus.google.com/112970180154389856347/about' rel='author'/>
<link href='https://plus.google.com/112970180154389856347' rel='me'/>

<!-- Facebook Meta Tag -->
<meta content='xxxxxxxxxxxxxxxxx' property='fb:app_id'/>
<meta content='xxxxxxxxxxxxxxxxx' property='fb:admins'/>
<meta content='xxxxxxxxxxxxxxxxx' property='fb:page_id'/>

<!-- Twitter Meta Tag -->
<b:if cond='data:blog.metaDescription'>
	<meta expr:content='data:blog.metaDescription' property='twitter:description'/>
	<b:else/>
	<!-- jika deskripsi tidak ditemukan boleh di isi sendiri -->
	<meta content='Tempat Situs Link dan Bookmark Seputar Bisnis Blogger, SEO, Website, dan Pemrograman, Ditambah Situs Situs Kesukaan Agar Tidak Hilang' property='twitter:description'/>
</b:if>
<b:if cond='data:blog.url == &quot;index&quot;'>
	<meta expr:content='data:blog.title' property='twitter:title'/>
</b:if>
<b:if cond='data:blog.pageType == &quot;item&quot;'>
	<meta expr:content='data:blog.pageName' property='twitter:title'/>
</b:if>
<meta expr:content='data:blog.title' name='twitter:site'/>
<meta content='summary' property='twitter:card'/>
<meta content='#######' property='twitter:account_id'/>
<meta content='#######' property='twitter:creator'/>

<!-- SEO Meta Tag -->
<b:if cond='data:blog.url == data:blog.homepageUrl'>
	<meta content='index,follow' name='robots'/>
	<meta content='index,follow' name='googlebot'/>
	<meta content='index,follow' name='googlebot-news'/>
</b:if>
<b:if cond='data:blog.pageType == &quot;index&quot;'>
	<b:if cond='data:blog.searchLabel'>
		<meta content='noindex,nofollow' name='robots'/>
	</b:if>
</b:if>
<b:if cond='data:blog.pageType == &quot;archive&quot;'>
	<meta content='noindex' name='robots'/>
</b:if>
