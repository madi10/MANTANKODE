<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
	&lt;head&gt;

	<!-- SEO Tittle -->
	<b:if cond='data:blog.url == data:blog.homepageUrl'>
		<title><data:blog.title/></title>
	</b:if>
	<b:if cond='data:blog.pageType == &quot;index&quot;'>
		<b:if cond='data:blog.searchLabel'>
			<title><data:blog.pageName/></title>
			<b:else/>
			<b:if cond='data:blog.url != data:blog.homepageUrl'>
				<b:if cond='data:blog.searchQuery'>
					<title><data:blog.searchQuery/> - <data:blog.title/></title>
					<b:else/>
					<title><data:blog.pageTitle/> - All Post</title>
				</b:if>
			</b:if>
		</b:if>
	</b:if>
	<b:if cond='data:blog.pageType == &quot;item&quot;'>
		<title><data:blog.pageName/> - <data:blog.title/></title> 
	</b:if>
	<b:if cond='data:blog.pageType == &quot;archive&quot;'>
		<title> Archive for <data:blog.pageName/></title> 
	</b:if>
	<b:if cond='data:blog.pageType == &quot;static_page&quot;'>
		<title><data:blog.pageName/></title>
	</b:if>
	<b:if cond='data:blog.pageType == &quot;error_page&quot;'>
		<title>Page Not Found</title>
	</b:if>

	<!-- include meta blogger -->
	<meta content='text/html; charset=UTF-8' http-equiv='Content-Type'/>
	<meta content='blogger' name='generator'/>
	<link expr:href='data:blog.homepageUrl + &quot;favicon.ico&quot;' rel='icon' type='image/x-icon'/>
	<link expr:href='data:blog.url' rel='canonical'/>
	<link expr:href='data:blog.homepageUrl + &quot;feeds/posts/default&quot;' expr:title='data:blog.title + &quot; - Atom&quot;' rel='alternate' type='application/atom+xml'/>
	<link expr:href='data:blog.homepageUrl + &quot;feeds/posts/default?alt=rss&quot;' expr:title='data:blog.title + &quot; - RSS&quot;' rel='alternate' type='application/rss+xml'/>
	<link expr:href='&quot;http://www.blogger.com/feeds/&quot; + data:blog.blogId + &quot;/posts/default&quot;' expr:title='data:blog.title + &quot; - Atom&quot;' rel='alternate' type='application/atom+xml'/>
	<link href='http://www.blogger.com/openid-server.g' rel='openid.server'/>
	<link expr:href='data:blog.homepageUrl' rel='openid.delegate'/>
	<b:if cond='data:blog.pageType == &quot;item&quot;'>
		<b:if cond='data:blog.postImageThumbnailUrl'>
			<link expr:href='data:blog.postImageThumbnailUrl' rel='image_src'/>
		</b:if>
		<b:if cond='data:blog.metaDescription != &quot;&quot;'>
			<meta expr:content='data:blog.metaDescription' name='description'/>
			<b:else/>
			<meta expr:content='data:blog.pageName + &quot; - &quot; + data:blog.title' name='description'/>
		</b:if>
	</b:if>

	<!-- Meta Description and Keyword -->
	<b:if cond='data:blog.pageType == &quot;item&quot;'>
		<!-- halaman item -->
		<b:if cond='data:blog.metaDescription'>
			<meta expr:content='data:blog.metaDescription' name='description'/>
			<b:else/>
			<!-- jika deskripsi tidak ditemukan boleh di isi sendiri -->
			<meta content='Tempat Situs Link dan Bookmark Seputar Bisnis Blogger, SEO, Website, dan Pemrograman, Ditambah Situs Situs Kesukaan Agar Tidak Hilang' name='description'/>
		</b:if>
		<b:if cond='data:blog.postImageThumbnailUrl'>
			<link expr:href='data:blog.postImageThumbnailUrl' rel='image_src'/>
		</b:if>
		<meta expr:content='data:blog.pageName' name='keywords'/>
		<b:else/>
		<!-- bukan halaman item -->
		<!-- halaman search -->
		<b:if cond='data:blog.searchQuery'>
			<meta expr:content='data:blog.searchQuery' name='description'/>
			<meta expr:content='data:blog.searchQuery' name='keywords'/>
			<meta content='index,follow,noodp,noydir' name='robots'/>
			<meta content='1 days' name='revisit-after'/>
			<meta content='always' name='revisit'/>
			<b:else/>
			<!-- bukan halaman item dan search (homepage,dll)  -->
			<b:if cond='data:blog.metaDescription'>
				<meta expr:content='data:blog.metaDescription' name='description'/>
				<b:else/>
				<!-- jika deskripsi tidak ditemukan boleh di isi sendiri -->
				<meta content='Tempat Situs Link dan Bookmark Seputar Bisnis Blogger, SEO, Website, dan Pemrograman, Ditambah Situs Situs Kesukaan Agar Tidak Hilang' name='description'/>
			</b:if>
			<meta content='Tempat Situs Link dan Bookmark Seputar Bisnis Blogger, SEO, Website, dan Pemrograman, Ditambah Situs Situs Kesukaan Agar Tidak Hilang' name='keywords'/>       
		</b:if>
	</b:if>

	<!-- Meta Indentification -->
	<meta charset='utf-8'/>
	<meta content='width=device-width, initial-scale=1' name='viewport'/>
	<meta content='blogger' name='generator'/>
	<meta content='text/html; charset=UTF-8' http-equiv='Content-Type'/>
	<link href='http://www.blogger.com/openid-server.g' rel='openid.server'/>
	<link expr:href='data:blog.homepageUrl' rel='openid.delegate'/>
	<meta content='IE=edge,chrome=1' http-equiv='X-UA-Compatible'/>
	<meta content='all-language' http-equiv='Content-Language'/>
	<meta content='Indonesia' name='geo.placename'/>
	<meta content='xxxxxxxxxxxxxxxxx' name='Author'/>
	<meta content='general' name='rating'/>
	<meta content='id' name='geo.country'/>

<!-- BEGIN Open Graph tags 
taruh kode : xmlns:og='http://ogp.me/ns#' 
dibagian <html>
